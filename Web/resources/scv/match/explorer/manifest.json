{
	"_version": "1.0.0",
	"sap.app": {
		"id": "osr.scv.match.explorer",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "0.0.1"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"SCVExplorerRemote": {
				"uri": "/scv/match/srv/xs/explorer/scvExplorer.xsodata",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			},
			"AdvanceFilterParametersJSON": {
				"uri": "model/filterParameters.json",
				"type": "JSON",
				"settings": {}
			}
		},
		"offline": true
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "osr.scv.match.explorer.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.52.6",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "osr.scv.match.explorer.i18n.i18n"
				}
			},
			"scvExplorerModel": {
				"dataSource": "SCVExplorerRemote",
				"settings": {
					"disableHeadRequestForToken": "false",
					"defaultCountMode": "None"
				},
				"preload": true
			},
			"advanceFilterModel": {
				"dataSource": "AdvanceFilterParametersJSON",
				"settings": {
					"disableHeadRequestForToken": "false",
					"defaultCountMode": "None"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}],
			"js": [{
				"uri": "asset/lib/moment-with-locales.js"
			}, {
				"uri": "asset/lib/moment-business-days.js"
			}, {
				"uri": "asset/lib/bluebird.min.js"
			}]
		},
		"config": {
			"unstructuredSearch": "/scv/match/srv/xs/explorer/unstructuredSearch.xsjs"
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "osr.scv.match.explorer.view",
				"controlAggregation": "pages",
				"controlId": "appRootControl"
			},
			"routes": [{
				"pattern": "",
				"name": "homepage",
				"target": "home"
			}, {
				"pattern": "home",
				"name": "appHome",
				"target": "home"
			}, {
				"pattern": "objectdetail/{scvId}",
				"name": "objectdetail",
				"target": "objectdetail"
			}],
			"targets": {
				"home": {
					"viewType": "XML",
					"viewName": "MainMenu",
					"viewLevel": 0,
					"controlAggregation": "pages"
				},
				"objectdetail": {
					"viewType": "XML",
					"viewName": "DetailObject",
					"viewLevel": 0,
					"controlAggregation": "pages"
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}