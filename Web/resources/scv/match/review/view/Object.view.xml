<mvc:View controllerName="osr.scv.match.review.controller.Object" xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.m.semantic"
	xmlns:form="sap.ui.layout.form" xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:t="sap.ui.table">
	<semantic:FullscreenPage id="page" busy="{objectView>/busy}" busyIndicatorDelay="{objectView>/delay}" navButtonPress="onNavBack"
		showNavButton="true" title="{i18n>objectTitle} { path: 'ENTITY_ID' }">
		<semantic:content>
			<ObjectHeader fullScreenOptimized="false" id="objectHeader" responsive="true" title="{NAME}" numberUnit="%"
				numberState="{ path: 'AVG_SCORE', formatter: '.formatter.scoreState' }" number="{AVG_SCORE}">
				<attributes>
					<!--<ObjectAttribute id="oaGroupId" title="Group ID" text="{ path: 'GROUP_ID' }"/>-->
					<ObjectAttribute id="oaEntityId" title="Entity ID" text="{ path: 'ENTITY_ID' }"/>
					<ObjectAttribute title="Source System" text="{ path: 'SOURCE_SYSTEM'}"/>
				</attributes>
				<statuses>
					<ObjectStatus title="Strategy" text="{= ${STRATEGY_RESOLVED} }" state="{= ${STRATEGY_RESOLVED_STATUS} }" visible="true"/>
					<ObjectStatus title="Action" text="{= ${ACTION_RESOLVED} }" state="{= ${ACTION_RESOLVED_STATUS} }" visible="true"/>
					<!--<ProgressIndicator percentValue="{UnitsInStock}" displayValue="{UnitsInStock}" showValue="true"
						state="{ path: 'UnitsInStock', formatter: '.formatter.quantityState'}"/>-->
				</statuses>
				<headerContainer>
					<IconTabBar id="itb1" selectedKey="key1" upperCase="true" expandable="false" class="sapUiResponsiveContentPadding">
						<items>
							<IconTabFilter text="Entity Info" key="key1">
								<content>
									<t:Table id="table" rows="{ path: 'matchResults', sorter: { path: 'GROUP_ROW_COUNT', descending: false}}" ariaLabelledBy="title"
										selectionMode="MultiToggle" rowSelectionChange="boxTickedEvent" enableBusyIndicator="true" busyIndicatorDelay="50">
										<t:toolbar>
											<Toolbar>
												<Title id="tableDetailsHeader" text="Match Group Details"/>
												<ToolbarSpacer/>
												<Button id="acceptButton" type="Accept" text="Save Entity" press="onOpenAcceptDialog"
													enabled="{= ${STRATEGY_RESOLVED} !== 'System Exclude'}"/>
												<!--<Button id="rejectButton" type="Reject" text="Reject" press="onOpenRejectDialog"/>-->
												<!--<Button id="promoteButton" type="Accept" text="Promote" enabled="{= ${STRATEGY_RESOLVED}.match('Review') ? true : false}" pressed="true"
													press="onPromote" visible="{= ${STRATEGY_RESOLVED}.match('Review') ? true : false}"/>
												<Button id="rejectButton" type="Reject" text="Reject" enabled="{= ${STRATEGY_RESOLVED}.match('Promote') ? true : false}" pressed="true"
													press="onReject" visible="{= ${STRATEGY_RESOLVED}.match('Promote') ? true : false}"/>-->
											</Toolbar>
										</t:toolbar>
										<t:columns>
											<!--	<t:Column width="5rem">
												<t:template>
													<Switch id="switch" type="AcceptReject" state="true" enabled="true" tooltip="Accept or Reject row" change="onSwitchChange" />
												</t:template>
											</t:Column>-->
											<t:Column width="3rem">
												<Label text="#"/>
												<t:template>
													<Text text="{GROUP_ROW_COUNT}" wrapping="false"/>
												</t:template>
											</t:Column>
											<t:Column width="5rem">
												<Label text="Status"/>
												<t:template>
													<Text text="{ACTION}" wrapping="false"/>
												</t:template>
											</t:Column>
											<t:Column width="5rem">
												<Label text="SCV ID"/>
												<t:template>
													<Text text="{SCV_ID}" wrapping="false"/>
												</t:template>
											</t:Column>
											<t:Column width="8rem">
												<Label text="System"/>
												<t:template>
													<Text text="{SOURCE_SYSTEM}" wrapping="false"/>
												</t:template>
											</t:Column>
											<t:Column width="8rem">
												<Label text="System ID"/>
												<t:template>
													<Text text="{SYSTEM_ID}" wrapping="false"/>
												</t:template>
											</t:Column>
											<t:Column width="8rem">
												<Label text="Date of Birth"/>
												<t:template>
													<Text text="{BIRTHDT}" wrapping="false"/>
												</t:template>
											</t:Column>
											<t:Column width="12rem">
												<Label text="Last Name"/>
												<t:template>
													<Text text="{LAST_NAME}" wrapping="false"/>
												</t:template>
											</t:Column>
											<t:Column width="18rem">
												<Label text="First Middle Name"/>
												<t:template>
													<Text text="{FIRST_MIDDLE_NAME}" wrapping="false"/>
												</t:template>
											</t:Column>
											<t:Column width="auto">
												<Label text="Address"/>
												<t:template>
													<Text text="{ADDRESS}" wrapping="false"/>
												</t:template>
											</t:Column>
											<!--<t:Column>
												<Label text="Reqion"/>
												<t:template>
													<Text text="{ADDRESS_REGION}" wrapping="false"/>
												</t:template>
											</t:Column>
											<t:Column>
												<Label text="Post Code"/>
												<t:template>
													<Text text="{ADDRESS_POSTCODE}" wrapping="false"/>
												</t:template>
											</t:Column>
											<t:Column>
												<Label text="City"/>
												<t:template>
													<Text text="{ADDRESS_LOCALITY}" wrapping="false"/>
												</t:template>
											</t:Column>
											<t:Column>
												<Label text="Street"/>
												<t:template>
													<Text text="{ADDRESS_PRIM_NAME}" wrapping="false"/>
												</t:template>
											</t:Column>
											<t:Column>
												<Label text="Street Number"/>
												<t:template>
													<Text text="{ADDRESS_PRIM_NUMBER}" wrapping="false"/>
												</t:template>
											</t:Column>-->
											<t:Column width="8rem">
												<Label text="Match Row"/>
												<t:template>
													<Text text="{MATCH_ROW}" wrapping="false"/>
												</t:template>
											</t:Column>
											<t:Column id="navigation" width="3rem">
												<Label text=""/>
												<t:template>
													<Button icon="sap-icon://target-group" press="onPress2"/>
												</t:template>
											</t:Column>
										</t:columns>
									</t:Table>
									<l:VerticalLayout class="sapUiContentPadding" width="100%">
										<l:content>
											<core:HTML content='&lt;div class=&quot;content&quot;&gt;&lt;h4&gt;&lt;/a&gt;&lt;/div&gt;'></core:HTML>
										</l:content>
									</l:VerticalLayout>
									<t:Table id="detailsTable1"
										rows="{ path: '/matchResultsDetailsRelatedParameters(I_MATCH_ROW=\'1\')/Results', sorter: [{ path: 'POLICY_NAME', descending: true },{ path: 'SCORE', descending: false }]}"
										ariaLabelledBy="title" selectionMode="None" enableBusyIndicator="true" busyIndicatorDelay="50">
										<t:toolbar>
											<Toolbar>
												<Title id="tableDetails1Header" text="Matches for Row "/>
												<ToolbarSpacer/>
											</Toolbar>
										</t:toolbar>
										<t:columns>
											<t:Column width="18rem">
												<Label text="Policy Name"/>
												<t:template>
													<Text text="{POLICY_NAME}" wrapping="false"/>
												</t:template>
											</t:Column>
											<t:Column width="4rem">
												<Label text="Score"/>
												<t:template>
													<ObjectNumber number="{SCORE}" state="{ path: 'SCORE', formatter: '.formatter.matchState' }"/>
												</t:template>
											</t:Column>
											<t:Column width="8rem">
												<Label text="System"/>
												<t:template>
													<Text text="{SOURCE_SYSTEM}" wrapping="false"/>
												</t:template>
											</t:Column>
											<t:Column width="8rem">
												<Label text="System ID"/>
												<t:template>
													<Text text="{SYSTEM_ID}" wrapping="false"/>
												</t:template>
											</t:Column>
											<t:Column width="8rem">
												<Label text="Date of Birth"/>
												<t:template>
													<Text text="{BIRTHDT}" wrapping="false"/>
												</t:template>
											</t:Column>
											<t:Column width="12rem">
												<Label text="Last Name"/>
												<t:template>
													<Text text="{LAST_NAME}" wrapping="false"/>
												</t:template>
											</t:Column>
											<t:Column width="18rem">
												<Label text="First Middle Name"/>
												<t:template>
													<Text text="{FIRST_MIDDLE_NAME}" wrapping="false"/>
												</t:template>
											</t:Column>
											<t:Column width="auto">
												<Label text="Address"/>
												<t:template>
													<Text text="{ADDRESS}" wrapping="false"/>
												</t:template>
											</t:Column>
											<!--<t:Column>
												<Label text="Region"/>
												<t:template>
													<Text text="{ADDRESS_REGION}" wrapping="false"/>
												</t:template>
											</t:Column>
											<t:Column>
												<Label text="Postcode"/>
												<t:template>
													<Text text="{ADDRESS_POSTCODE}" wrapping="false"/>
												</t:template>
											</t:Column>
											<t:Column>
												<Label text="City"/>
												<t:template>
													<Text text="{ADDRESS_LOCALITY}" wrapping="false"/>
												</t:template>
											</t:Column>
											<t:Column>
												<Label text="Street"/>
												<t:template>
													<Text text="{ADDRESS_PRIM_NAME}" wrapping="false"/>
												</t:template>
											</t:Column>
											<t:Column>
												<Label text="Street Number"/>
												<t:template>
													<Text text="{ADDRESS_PRIM_NUMBER}" wrapping="false"/>
												</t:template>
											</t:Column>-->
											<t:Column width="8rem">
												<Label text="Match Row"/>
												<t:template>
													<Text text="{MATCH_ROW}" wrapping="false"/>
												</t:template>
											</t:Column>
										</t:columns>
									</t:Table>
									<!--	</content>
									</Panel>-->
								</content>
							</IconTabFilter>
							<IconTabFilter text="Change Log" key="key2" visible="false">
								<content>
									<l:VerticalLayout class="sapUiContentPadding" width="100%">
										<l:content>
											<t:Table id="changeLogTable" rows="{ path: 'matchAssessments', sorter: { path: 'TIMESTAMP', descending: true }}" ariaLabelledBy="title"
												selectionMode="None" enableGrouping="true">
												<t:columns>
													<t:Column width="10rem">
														<Label text="Strategy"/>
														<t:template>
															<Text text="{STRATEGY}" wrapping="false"/>
														</t:template>
													</t:Column>
													<t:Column width="10rem">
														<Label text="User ID"/>
														<t:template>
															<Text text="{USER}" wrapping="false"/>
														</t:template>
													</t:Column>
													<t:Column width="15rem">
														<Label text="Timestamp"/>
														<t:template>
															<Text text="{path: 'TIMESTAMP', type: 'sap.ui.model.type.DateTime'}" wrapping="false"/>
														</t:template>
													</t:Column>
													<t:Column width="12rem">
														<Label text="Match Row"/>
														<t:template>
															<Text text="{MATCH_ROW}" wrapping="false"/>
														</t:template>
													</t:Column>
													<t:Column width="10rem">
														<Label text="Action"/>
														<t:template>
															<ObjectStatus text="{ACTION}" state="{ path: 'ACTION', formatter: '.formatter.actionState' }" wrapping="false"/>
														</t:template>
													</t:Column>
													<t:Column width="18rem">
														<Label text="Code"/>
														<t:template>
															<Text text="{CODE}" wrapping="false"/>
														</t:template>
													</t:Column>
													<t:Column width="auto">
														<Label text="Comment"/>
														<t:template>
															<Text text="{COMMENT}" wrapping="false"/>
														</t:template>
													</t:Column>
												</t:columns>
											</t:Table>
										</l:content>
									</l:VerticalLayout>
								</content>
							</IconTabFilter>
						</items>
					</IconTabBar>
				</headerContainer>
			</ObjectHeader>
		</semantic:content>
		<!--<semantic:sendEmailAction>
			<semantic:SendEmailAction id="shareEmail" press="onShareEmailPress"/>
		</semantic:sendEmailAction>-->
	</semantic:FullscreenPage>
</mvc:View>