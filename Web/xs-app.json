{
	"welcomeFile": "webapp/index.html",
	"authenticationMethod": "route",
	"routes": [{
		"source": "/webapp/rest/.*",
		"destination": "Services_url",
		"scope": "$XSAPPNAME.VIEW"
	}, {
		"source": "(.*)(.xsjs)",
		"destination": "Services_url",
		"csrfProtection": false,
		"authenticationType": "xsuaa",
		"scope": "$XSAPPNAME.VIEW"
	}, {
		"source": "(.*)(.xsodata)",
		"destination": "Services_url",
		"authenticationType": "xsuaa",
		"scope": "$XSAPPNAME.VIEW"
	}, {
		"source": "^/(.*)$",
		"localDir": "resources",
		"scope": "$XSAPPNAME.VIEW",
		"replace": {
			"pathSuffixes": ["index.html", "odataTest.html"],
			"vars": ["ui5liburl"]
		}
	}]
}