namespace osr.scv.foundation.db.data;

context MatchResultsReview {

    /*@@layout{"layoutInfo":{}}*/
    entity Assessments {
        key ENTITY_ID : String(10);
        key TIMESTAMP : UTCTimestamp;
        key MATCH_ROW : Integer default 0;
            STRATEGY  : String(10);
            ACTION    : String(20);
            CODE      : String(60);
            COMMENT   : String(512);
            USER      : String(200);
    };

    entity Promotions {
        key ENTITY_ID : String(10);
            TIMESTAMP : UTCTimestamp;
            USER      : String(200);
            ACTION    : String(20);
            STRATEGY  : String(20); 
    };

    /*@@layout{"layoutInfo":{}}*/
    entity AssessmentsHistory {
        key SCV_LOAD_ID : Integer;
        key ENTITY_ID   : String(10);
        key TIMESTAMP   : UTCTimestamp;
        key MATCH_ROW   : Integer default 0;
            STRATEGY    : String(10);
            ACTION      : String(20);
            CODE        : String(60);
            COMMENT     : String(512);
            USER        : String(200);
    };

    entity PromotionsHistory {
        key SCV_LOAD_ID : Integer;
        key ENTITY_ID   : String(10);
            TIMESTAMP   : UTCTimestamp;
            USER        : String(200);
            STRATEGY    : String(20);
            ACTION      : String(20);
    };

    entity Review {
        key MATCH_ROW                : Integer;
        key SOURCE_SYSTEM            : String(10);
        key SYSTEM_ID                : String(10);
            ENTITY_ID                : String(20);
            GROUP_ID                 : Integer;
            REVIEW_GROUP             : String(1);
            GROUP_COUNT              : Integer;
            GROUP_ROW_COUNT          : Integer;
            GROUP_CATEGORY           : Integer;
            FIRST_NAME               : String(50);
            MIDDLE_NAME              : String(50);
            LAST_NAME                : String(80);
            RELATED_ROW              : Integer;
            SCORE                    : Integer;
            AVG_SCORE                : Integer;
            STRATEGY                 : String(10);
            STRATEGY_RESOLVED        : String(20);
            STRATEGY_RESOLVED_STATUS : String(10);
            PROMOTED_TO_SCV          : String(1);
            PROMOTION_TIMESTAMP      : UTCTimestamp;
            PROMOTED_BY              : String(10);
            RMS_DUPLICATES           : Integer;
            ACTION                   : String(20);
            ACTION_RESOLVED          : String(20);
            MATCH_PERSON_FN          : String(50);
            ACTION_RESOLVED_STATUS   : String(10);
            SHOW_IN_REVIEW           : String(1);
    }
    technical configuration {
        index index_0 on (PROMOTED_TO_SCV);
    };

    entity ReviewShadow {
        key MATCH_ROW                : Integer;
        key SOURCE_SYSTEM            : String(10);
        key SYSTEM_ID                : String(10);
            ENTITY_ID                : String(20);
            GROUP_ID                 : Integer;
            REVIEW_GROUP             : String(1);
            GROUP_COUNT              : Integer;
            GROUP_ROW_COUNT          : Integer;
            GROUP_CATEGORY           : Integer;
            FIRST_NAME               : String(50);
            MIDDLE_NAME              : String(50);
            LAST_NAME                : String(80);
            RELATED_ROW              : Integer;
            SCORE                    : Integer;
            AVG_SCORE                : Integer;
            STRATEGY                 : String(10);
            STRATEGY_RESOLVED        : String(10);
            STRATEGY_RESOLVED_STATUS : String(10);
            PROMOTED_TO_SCV          : String(1);
            PROMOTION_TIMESTAMP      : UTCTimestamp;
            PROMOTED_BY              : String(10);
            RMS_DUPLICATES           : Integer;
            ACTION                   : String(20);
            ACTION_RESOLVED          : String(20);
            MATCH_PERSON_FN          : String(80);
            ACTION_RESOLVED_STATUS   : String(10);
    }
    technical configuration {
        index index_0 on (PROMOTED_TO_SCV);
    };

    type StringValues {
        STRING_VALUE : String(50);
    };

    /*@@layout{"layoutInfo":{"x":-737,"y":-69.5}}*/
    entity AssessmentTexts {
        key CODE        : String(60);
            DESCRIPTION : String(256);
    };
};